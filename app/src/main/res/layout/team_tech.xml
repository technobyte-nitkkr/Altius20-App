<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:padding="20sp"
    app:layout_goneMarginBottom="15sp"
    android:layout_height="wrap_content"
    tools:context=".drawers.developers.Developers"
    android:background="@drawable/edittextround">
    <ScrollView
        android:layout_marginTop="10sp"
        android:id="@+id/guest_picker"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/buttonbackground"
        android:backgroundTint="#000"
        android:fillViewport="true"
        >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:id="@+id/sname"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:layout_marginBottom="5sp"
            android:fontFamily="@font/aclonica"
            android:textColor="@color/txt_color"
            android:textSize="28sp"
            android:textStyle="bold" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="100">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_weight="50"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:weightSum="3">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:orientation="vertical">
                    <de.hdodenhof.circleimageview.CircleImageView
                        app:civ_border_width="5sp"
                        app:civ_border_color="@color/altius_green"
                        android:padding="5sp"
                        android:layout_gravity="center"
                        android:src="@drawable/deafultpic"
                        android:layout_width="120sp"
                        android:layout_height="120sp"
                        android:id="@+id/img1"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/name1"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="15sp"
                        android:text="President"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/post1"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/team_p1_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10sp"
                    android:layout_margin="10sp"
                    android:orientation="vertical">
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:padding="2sp"
                        app:civ_border_width="5sp"
                        app:civ_border_color="@color/altius_green"
                        android:src="@drawable/deafultpic"
                        android:layout_width="120sp"
                        android:layout_height="120sp"
                        android:layout_gravity="center"
                        android:id="@+id/img3"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/name3"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="15sp"
                        android:text="President"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/post3"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10sp"
                    android:visibility="gone"
                    android:id="@+id/p5"
                    android:layout_margin="10sp"
                    android:orientation="vertical">
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:padding="5sp"
                        app:civ_border_width="5sp"
                        app:civ_border_color="@color/altius_green"
                        android:layout_width="120sp"
                        android:layout_height="120sp"
                        android:layout_gravity="center"
                        android:src="@drawable/deafultpic"
                        android:id="@+id/img5"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/name5"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="15sp"
                        android:text="President"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/post5"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:id="@+id/team_p2"
                android:layout_width="match_parent"
                android:layout_weight="50"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:weightSum="3">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10sp"
                    android:orientation="vertical">
                    <de.hdodenhof.circleimageview.CircleImageView
                        app:civ_border_width="5sp"
                        app:civ_border_color="@color/altius_green"
                        android:padding="5sp"
                        android:layout_gravity="center"
                        android:src="@drawable/deafultpic"
                        android:layout_width="120sp"
                        android:layout_height="120sp"
                        android:id="@+id/img2"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/name2"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="15sp"
                        android:text="President"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/post2"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="10sp"
                    android:orientation="vertical">
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:padding="2sp"
                        app:civ_border_width="5sp"
                        app:civ_border_color="@color/altius_green"
                        android:src="@drawable/deafultpic"
                        android:layout_width="120sp"
                        android:layout_height="120sp"
                        android:layout_gravity="center"
                        android:id="@+id/img4"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/name4"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="15sp"
                        android:text="President"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/post4"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="10sp"
                    android:layout_margin="10sp"
                    android:visibility="gone"
                    android:id="@+id/p6"
                    android:orientation="vertical">
                    <de.hdodenhof.circleimageview.CircleImageView
                        android:padding="5sp"
                        app:civ_border_width="5sp"
                        app:civ_border_color="@color/altius_green"
                        android:layout_width="120sp"
                        android:layout_height="120sp"
                        android:layout_gravity="center"
                        android:src="@drawable/deafultpic"
                        android:id="@+id/img6"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="16sp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/name6"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                    <TextView
                        android:textStyle="bold"
                        android:textSize="15sp"
                        android:text="President"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:id="@+id/post6"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:textColor="@color/txt_color"/>
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
    </ScrollView>


</LinearLayout>